{
  "name": "wingman-vscode",
  "publisher": "adrianliechti",
  "displayName": "Wingman AI",
  "description": "Wingman AI",
  "version": "0.0.8",
  "icon": "assets/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/adrianliechti/wingman-vscode"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "AI",
    "Chat"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Wingman",
      "properties": {
        "wingman.baseUrl": {
          "type": "string",
          "default": "http://localhost:4242/v1",
          "scope": "machine"
        },
        "wingman.apiKey": {
          "type": "string",
          "default": "",
          "scope": "machine"
        }
      }
    },
    "languageModelChatProviders": [
      {
        "vendor": "wingman",
        "displayName": "Wingman"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.3",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "openai": "^6.15.0"
  }
}
